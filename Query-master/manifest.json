{
  "name": "Query",
  "author": "Akira HIRATA",
  "version": "0.5.0",
  "manifest_version": 2,
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "docs/images/icon16.png",
    "48": "docs/images/icon48.png",
    "128": "docs/images/icon128.png"
  },
  "background": {
    "scripts": [
      "docs/scripts/analytics.min.js",
      "docs/scripts/background.min.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "docs/main.html"
  },
  "options_page": "docs/options.html",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "minimum_chrome_version": "22",
  "homepage_url": "https://github.com/psephopaiktes/Query"
}
